package polimorfismo;

/**
 *
 * @author DELL
 */
public class Cuadrado extends Figura {
    private double lado;
    @Override
    
    public double calcularArea() {
      double factor=this.convertirMilimetros();
      return factor*(lado*lado);
    }
/////getter constructor and setter
    public double getLado() {
        return lado;
    }

    public void setLado(double lado) {
        this.lado = lado;
    }

    public Cuadrado(double lado) {
        this.lado = lado;
    }
    
}
