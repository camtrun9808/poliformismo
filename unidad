package polimorfismo;

/**
 *
 * @author DELL
 */
public enum UnidadMedida {
    
MIILIMETROS(1),
CENTIMETROS(100), 
METROS(1000);

private double factor;

    UnidadMedida(double factor) {
        this.factor = factor;
    }

    public double getFactor() {
        return factor;
    }


}
